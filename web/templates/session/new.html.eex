<h1>Login</h1>
<%= form_for @conn, session_path(@conn, :create), [as: :session], fn f -> %>
  <div class="">
    <%= label f, :email, class: "" %>
    <%= text_input f, :email, class: "" %>
  </div>
  <div class="">
    <%= label f, :password, class: "" %>
    <%= password_input f, :password, class: "" %>
  </div>
  <%= submit "Log in", class: "" %>
<% end %>
